{
  "name": "owlcms-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js dev --port 8096",
    "dev:learning": "cross-env LEARNING_MODE=true node node_modules/vite/bin/vite.js dev --port 8096",
    "build": "svelte-kit sync && vite build && node scripts/copy-server.js",
    "build:windows": "npm run build && node scripts/build-windows.js",
    "build:macos": "npm run build && node scripts/build-macos.js",
    "build:rpi": "npm run build && node scripts/build-rpi.js",
    "build:all": "npm run build:windows && npm run build:macos && npm run build:rpi",
    "preview": "vite preview --port 8096",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "release": "node scripts/release.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "cross-env": "^7.0.3",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^4.0.13"
  },
  "type": "module",
  "dependencies": {
    "@owlcms/tracker-core": "github:owlcms/tracker-core#1.0.0-rc01",
    "adm-zip": "^0.5.10",
    "gsap": "^3.13.0",
    "puppeteer-core": "^24.34.0",
    "ws": "^8.18.3"
  }
}

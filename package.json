{
  "name": "owlcms-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js dev --port 8096",
    "dev:learning": "cross-env LEARNING_MODE=true node node_modules/vite/bin/vite.js dev --port 8096",
    "build": "vite build",
    "preview": "vite preview --port 8096",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "cross-env": "^7.0.3",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^4.0.13"
  },
  "type": "module",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "gsap": "^3.13.0",
    "ws": "^8.18.3"
  }
}
